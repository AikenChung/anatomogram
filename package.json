{
  "name": "anatomogram",
  "version": "1.2.3",
  "description": "Anatomogram component for Expression Atlas heatmap",
  "main": "lib/index.js",
  "scripts": {
    "parseSvgs": "node src/utils/svgParse.js src/svg src/json/svgsMetadata.json ",
    "build": "yarn run parseSvgs && rm -rf lib && babel src -d lib --ignore '**/*.less','**/*.sass' --copy-files",
    "dist": "NODE_ENV=production webpack --debug --optimize-minimize --output-pathinfo --display-chunks --display-modules",
    "coverage": "jest --coverage",
    "test": "yarn run coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "prepublish": "yarn run coverage && yarn run build"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "author": "Alfonso Mu√±oz-Pomer Fuentes <amunoz@ebi.ac.uk>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/gxa/atlas-anatomogram.git"
  },
  "dependencies": {
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-svg": "^2.1.21",
    "urijs": "^1.18.10"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.16",
    "copy-webpack-plugin": "^4.0.1",
    "coveralls": "^2.13.1",
    "css-loader": "^0.28.4",
    "enzyme": "^2.9.1",
    "fast-xml-parser": "^2.6.0",
    "jest": "^20.0.4",
    "react-test-renderer": "^15.6.1",
    "sinon": "^2.3.8",
    "style-loader": "^0.18.2",
    "webpack": "^3.1.0",
    "webpack-dev-server": "^2.5.0"
  }
}
